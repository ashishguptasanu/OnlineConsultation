<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="ring"
            android:thickness="110dp"
            android:innerRadius="60dp"
            android:useLevel="false">
            <size
                android:width="500dp"
                android:height="500dp" />
            <solid android:color="#ffffff" />
        </shape>

    </item>
    <item
        android:width="342dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="120">
            <shape
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="line">
                <size tools:ignore="ExtraText">
                    android:width="0dp"
                    android:height="0dp"
                </size>
                <stroke
                    android:width="1dp"
                    android:color="#46000000"/>
            </shape>
        </rotate>
    </item>
    <item
        android:width="342dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="240">
            <shape
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="line">
                <size tools:ignore="ExtraText">
                    android:width="0dp"
                    android:height="0dp"
                </size>
                <stroke
                    android:width="1dp"
                    android:color="#46000000" />
            </shape>
        </rotate>
    </item>
    <item
        android:width="342dp"
        android:gravity="center">
        <rotate
            android:fromDegrees="0">
            <shape
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="line">
                <size tools:ignore="ExtraText">
                    android:width="0dp"
                    android:height="0dp"
                </size>
                <stroke
                    android:width="1dp"
                    android:color="#46000000" />
            </shape>
        </rotate>
    </item>
    <item
        android:height="125dp"
        android:width="125dp"
        android:gravity="center">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <!-- Add Color for background-->
            <solid android:color="#025d89" />
            <!-- Add Color and width for border-->
            <stroke

                android:width="1dp"
                android:color="#025d89" />
            <size android:width="125dp"
                android:height="125dp"/>
            <!-- add radious for rounded shape-->
            <corners
                android:bottomLeftRadius="2dp"
                android:bottomRightRadius="2dp"
                android:topLeftRadius="2dp"
                android:topRightRadius="2dp" />
        </shape>
    </item>
    <item
        android:height="100dp"
        android:width="100dp"
        android:gravity="center">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <!-- Add Color for background-->
            <solid android:color="#58b8e2" />
            <!-- Add Color and width for border-->
            <stroke
                android:width="1dp"
                android:color="#58b8e2" />
            <size android:height="100dp"
                android:width="100dp"/>
            <!-- add radious for rounded shape-->
            <corners
                android:bottomLeftRadius="2dp"
                android:bottomRightRadius="2dp"
                android:topLeftRadius="2dp"
                android:topRightRadius="2dp" />
        </shape>
    </item>

</layer-list>